{
  "plugins": ["stylelint-declaration-block-no-ignored-properties", "stylelint-no-unsupported-browser-features"],
  "extends": ["stylelint-config-standard","stylelint-config-recommended-scss", "stylelint-config-prettier", "stylelint-config-recess-order", "stylelint-prettier/recommended"],
  "rules": {
    "plugin/no-unsupported-browser-features": [true, {
      "browsers": ["> 5% in JP", "Last 2 Chrome versions", "Last 2 ChromeAndroid versions"],
      "ignore": ["rem"],
      "ignorePartialSupport": true
    }]
  }
}
